{
    "cli": {
        "version": ">=3.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "development"
            },
            "android": {
                "resourceClass": "medium",
                "android": {
                    "gradleCommand": ":app:assembleDevelopmentDebug"
                }
            },
            "ios": {
                "resourceClass": "medium",
                "simulator": true
            }
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "android": {
                "gradleCommand": ":app:bundleProductionRelease"
            }
        }
    }
}
